VERNAC COMMAND EXTEND MeBi_Config_Reset CLASSIFIED AS QUERY
| ["MeBi" "Config" "Reset"] -> {Mebi_api.reset_config ()}
| ["MeBi" "Config" "Reset" "Bound"] -> {Mebi_api.reset_bounds ()}
| ["MeBi" "Config" "Reset" "WeakMode"] -> {Mebi_api.reset_weak_mode ()}
| ["MeBi" "Config" "Reset" "FailIf"] -> {Mebi_api.reset_fail_if ()}
| ["MeBi" "Config" "Reset" "Debug"] -> {Mebi_api.reset_debug ()}
| ["MeBi" "Config" "Reset" "Output"] -> {Mebi_api.reset_output ()}
END

VERNAC COMMAND EXTEND MeBi_Config_Bounds CLASSIFIED AS QUERY
| ["MeBi" "Config" "Bound" int(x)] -> {Mebi_api.set_bound x}
| ["MeBi" "Config" "Bound" int(x) "On" "States"] -> {Mebi_api.set_bounds x Model_info.States}
| ["MeBi" "Config" "Bound" int(x) "On" "Transitions"] -> {Mebi_api.set_bounds x Model_info.Transitions}
END 

VERNAC COMMAND EXTEND MeBi_Config_Weak CLASSIFIED AS QUERY
| ["MeBi" "Config" "WeakMode" "Enable"] -> {Mebi_api.set_weak_mode_enabled true}
| ["MeBi" "Config" "WeakMode" "Disable"] -> {Mebi_api.set_weak_mode_enabled false}

(* set the weak transitions -- bisim will use for both terms unless specified *)
| ["MeBi" "Config" "Weak" "As" "Option" constr(x)] -> {Mebi_api.set_weak_arg1 (Some (Mebi_api.Option x))}
| ["MeBi" "Config" "Weak" "As" constr(x) "Of" reference(y)] -> {Mebi_api.set_weak_arg1 (Some (Mebi_api.Custom (x, y)))}

(* same as above *)
| ["MeBi" "Config" "Weak1" "As" "Option" constr(x)] -> {Mebi_api.set_weak_arg1 (Some (Mebi_api.Option x))}
| ["MeBi" "Config" "Weak1" "As" constr(x) "Of" reference(y)] -> {Mebi_api.set_weak_arg1 (Some (Mebi_api.Custom (x, y)))}

(* for second lts *)
| ["MeBi" "Config" "Weak2" "As" "Option" constr(x)] -> {Mebi_api.set_weak_arg2 (Some (Mebi_api.Option x))}
| ["MeBi" "Config" "Weak2" "As" constr(x) "Of" reference(y)] -> {Mebi_api.set_weak_arg2 (Some (Mebi_api.Custom (x, y)))}
END 

VERNAC COMMAND EXTEND MeBi_Config_Fail CLASSIFIED AS QUERY
