VERNAC COMMAND EXTEND MeBiSim CLASSIFIED AS QUERY

|![ proof ] 
  [ "MeBiSim" "Solve" int(upper_bound) ] ->
  { fun ~pstate -> Mebi_proof.solve upper_bound pstate }

|![ proof ] 
  [ "MeBiSim" "Step" ] ->
  { fun ~pstate -> Mebi_tactics.update_proof_by_tactic pstate (Mebi_proof.step ()) }

|![ proof ] 
  [ "MeBiSim" "Begin" reference(a) constr(x)  
                "And" reference(b) constr(y)
                "Using" reference_list(rs) ] -> 
  { fun ~pstate -> 
    Mebi_wrapper.run ~keep_encoding:false ~fresh:true (
      Command.proof_intro pstate (x, a) (y, b) rs
    ) 
  }

|![ proof ] 
  [ "MeBiSim" "Begin" reference(a) constr(x)  
                "And" reference(b) constr(y) ] -> 
  { fun ~pstate -> 
    Mebi_wrapper.run ~keep_encoding:false ~fresh:true (
      Command.proof_intro pstate (x, a) (y, b) []
    ) 
  }

END

VERNAC COMMAND EXTEND MeBi_LTS CLASSIFIED AS QUERY
| ["MeBi" "LTS" constr(x) "Using" reference(r)] -> 
  {Mebi_wrapper.run (Command.run (Command.MakeModel (Command.LTS, (x, r))) [])}
  
| ["MeBi" "LTS" constr(x) "Using" reference(r) reference_list(rs)] -> 
  {Mebi_wrapper.run (Command.run (Command.MakeModel (Command.LTS, (x, r))) rs)}
END 

