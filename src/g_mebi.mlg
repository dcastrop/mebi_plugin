VERNAC COMMAND EXTEND MeBi_Divider CLASSIFIED AS QUERY
(* for outputting messages -- useful for sifting through compiler output *)
| ["MeBi" "Message" string(x)] -> {Logger.Default.show x}
| ["MeBi" "Debug"   string(x)] -> {Logger.Default.debug x}
| ["MeBi" "Divider"          ] -> {Logger.Default.show "\n- - - - - - - - - - - - - - - -"}
| ["MeBi" string(x) "Divider"] -> {Logger.Default.show (Printf.sprintf "%s\n- - - - - - - - - - - - - - - -" x)}
| ["MeBi" "Divider" string(x)] -> {Logger.Default.show (Printf.sprintf "\n- - - - - - - - - - - - - - - -\n%s" x)}
| ["MeBi" string(x) "Divider" string(y)] -> 
  {Logger.Default.show (Printf.sprintf "%s\n- - - - - - - - - - - - - - - -\n%s" x y)}
END 


